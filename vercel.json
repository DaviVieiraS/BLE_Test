{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "bluetooth=(self)"
        },
        {
          "key": "Feature-Policy",
          "value": "bluetooth 'self'"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 30
    }
  }
}
